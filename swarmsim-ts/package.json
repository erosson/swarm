{
  "name": "@swarmsim/ts",
  "version": "1.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "gen": "mkdir -p src/generated && protoc --ts_out src/generated --proto_path protos protos/persist.proto",
    "start": "parcel serve --target standalone-dev",
    "build": "parcel build --target coffee-embed",
    "watch": "parcel watch --target coffee-embed",
    "test": "jest"
  },
  "targets": {
    "standalone-dev": {
      "source": "src/index.html",
      "distDir": "dist/standalone-dev/",
      "includeNodeModules": true
    },
    "coffee-embed": {
      "source": "src/index.html",
      "distDir": "dist/coffee-embed/",
      "publicUrl": "/swarmsim-ts",
      "includeNodeModules": true
    }
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.8.2",
    "@swarmsim/tables": "file:../tables",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "jest": "^29.3.1",
    "parcel": "^2.8.1",
    "process": "^0.11.10",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/lz-string": "^1.3.34",
    "decimal.js": "^10.4.3",
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.20",
    "io-ts-types": "^0.5.19",
    "lodash": "^4.17.21",
    "lz-string": "^1.4.4",
    "monocle-ts": "^2.3.13",
    "newtype-ts": "^0.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.5"
  }
}
